    ###并行迭代
###zip 拉链功能
lst_a=[0,1,2,3]
lst_b=['a','b','c','d']
zip_lst=zip(lst_a,lst_b)
print("zip_lst=zip(lst_a,lst_b)->{}".format(zip_lst))
print("dict(zip_lst)->{}".format(dict(zip_lst)))

zip_lst=zip(lst_a,lst_b)
for a,b in zip_lst:
    print(a,b)
    
zip_lst=zip(lst_a,lst_b)
a,b=zip(*zip_lst)
print("a={},b={}".format(a,b))
